extends layout

block content
  h1= title
  p Here are the results of the tests for each video.
    - video_list.sort(function(a, b) {let textA = a.name.toUpperCase(); let textB = b.name.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
    each video in video_list
      li 
        a(href=video.url+'/votes') #{video.name} 
        |  (#{video.description})

    else
      li There are no videos.